{
  "info": {
    "name": "PetWell API Collection",
    "description": "Comprehensive API collection for PetWell backend, covering authentication, pet management, business operations, vaccines, documents, timeline, teams, messages, and profile management. Created for frontend integration.",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "Auth",
      "item": [
        {
          "name": "Register",
          "request": {
            "method": "POST",
            "header": [
              { "key": "Content-Type", "value": "application/json" }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\"name\":\"John Doe\",\"email\":\"john@example.com\",\"password\":\"Password123\",\"role\":\"individual\",\"location\":\"New York\",\"phone\":\"1234567890\",\"inviteCode\":\"optional-invite-code-for-sub-user\"}"
            },
            "url": "{{BASE_URL}}/api/auth/register",
            "description": "Register a new user (individual, admin, or sub-user). Controller: auth.controller.register. Returns JWT token."
          },
          "response": [
            { "name": "Success", "status": "OK", "code": 200, "body": "{\"status\":\"ok\",\"token\":\"<jwt-token>\"}" },
            { "name": "Error", "status": "Bad Request", "code": 400, "body": "{\"error\":\"Invalid role\"}" }
          ]
        },
        {
          "name": "Login",
          "request": {
            "method": "POST",
            "header": [
              { "key": "Content-Type", "value": "application/json" }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\"email\":\"john@example.com\",\"password\":\"Password123\"}"
            },
            "url": "{{BASE_URL}}/api/auth/login",
            "description": "Login user and get JWT token. Controller: auth.controller.login."
          },
          "response": [
            { "name": "Success", "status": "OK", "code": 200, "body": "{\"status\":\"ok\",\"token\":\"<jwt-token>\"}" },
            { "name": "Error", "status": "Unauthorized", "code": 401, "body": "{\"error\":\"Invalid credentials\"}" }
          ]
        },
        {
          "name": "Verify Token",
          "request": {
            "method": "GET",
            "header": [
              { "key": "x-access-token", "value": "{{TOKEN}}" }
            ],
            "url": "{{BASE_URL}}/api/auth/verify-token",
            "description": "Verify JWT token validity. Controller: auth.controller.verifyToken."
          },
          "response": [
            { "name": "Success", "status": "OK", "code": 200, "body": "{\"status\":\"ok\",\"user\":{\"id\":\"<user-id>\",\"email\":\"john@example.com\",\"role\":\"individual\"}}" },
            { "name": "Error", "status": "Unauthorized", "code": 401, "body": "{\"error\":\"No token provided\"}" }
          ]
        },
        {
          "name": "Google OAuth",
          "request": {
            "method": "GET",
            "url": "{{BASE_URL}}/api/auth/google",
            "description": "Initiate Google OAuth flow. Redirects to Google login page. Controller: passport.google."
          }
        },
        {
          "name": "Google OAuth Callback",
          "request": {
            "method": "GET",
            "url": "{{BASE_URL}}/api/auth/google/callback",
            "description": "Handle Google OAuth callback. Redirects to frontend with token. Controller: socialAuth.controller.handleGoogleCallback."
          }
        }
      ]
    },
    {
      "name": "Pets",
      "item": [
        {
          "name": "Create Pet",
          "request": {
            "method": "POST",
            "header": [
              { "key": "x-access-token", "value": "{{TOKEN}}" },
              { "key": "Content-Type", "value": "multipart/form-data" }
            ],
            "body": {
              "mode": "formdata",
              "formdata": [
                { "key": "name", "value": "Max", "type": "text" },
                { "key": "species", "value": "dog", "type": "text" },
                { "key": "breed", "value": "Labrador Retriever", "type": "text" },
                { "key": "age", "value": "3", "type": "text" },
                { "key": "weight", "value": "25", "type": "text" },
                { "key": "location", "value": "New York", "type": "text" },
                { "key": "spayNeuter", "value": "neutered", "type": "text" },
                { "key": "color", "value": "Yellow", "type": "text" },
                { "key": "dateOfBirth", "value": "2022-01-01", "type": "text" },
                { "key": "microchip", "value": "123456789", "type": "text" },
                { "key": "notes", "value": "Friendly dog", "type": "text" },
                { "key": "profilePicture", "type": "file", "src": "/path/to/image.jpg" }
              ]
            },
            "url": "{{BASE_URL}}/api/pets",
            "description": "Create a new pet profile (individual users only). Controller: pet.controller.createPet."
          },
          "response": [
            { "name": "Success", "status": "OK", "code": 200, "body": "{\"status\":\"ok\",\"pet\":{...}}" },
            { "name": "Error", "status": "Forbidden", "code": 403, "body": "{\"error\":\"Only individual users can create pet profiles\"}" }
          ]
        },
        {
          "name": "Get Pets",
          "request": {
            "method": "GET",
            "header": [
              { "key": "x-access-token", "value": "{{TOKEN}}" }
            ],
            "url": "{{BASE_URL}}/api/pets",
            "description": "Retrieve all pets for the authenticated user. Controller: pet.controller.getPets."
          },
          "response": [
            { "name": "Success", "status": "OK", "code": 200, "body": "{\"status\":\"ok\",\"pets\":[{...}]}" }
          ]
        },
        {
          "name": "Update Pet",
          "request": {
            "method": "PUT",
            "header": [
              { "key": "x-access-token", "value": "{{TOKEN}}" },
              { "key": "Content-Type", "value": "multipart/form-data" }
            ],
            "body": {
              "mode": "formdata",
              "formdata": [
                { "key": "name", "value": "Max Updated", "type": "text" },
                { "key": "profilePicture", "type": "file", "src": "/path/to/new-image.jpg" }
              ]
            },
            "url": "{{BASE_URL}}/api/pets/{{PET_ID}}",
            "description": "Update a pet profile. Controller: pet.controller.updatePet."
          },
          "response": [
            { "name": "Success", "status": "OK", "code": 200, "body": "{\"status\":\"ok\"}" },
            { "name": "Error", "status": "Not Found", "code": 404, "body": "{\"error\":\"Pet not found\"}" }
          ]
        },
        {
          "name": "Transfer Pet",
          "request": {
            "method": "POST",
            "header": [
              { "key": "x-access-token", "value": "{{TOKEN}}" },
              { "key": "Content-Type", "value": "application/json" }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\"newOwnerEmail\":\"newowner@example.com\"}"
            },
            "url": "{{BASE_URL}}/api/pets/{{PET_ID}}/transfer",
            "description": "Transfer pet ownership to another user. Controller: pet.controller.transferPet."
          },
          "response": [
            { "name": "Success", "status": "OK", "code": 200, "body": "{\"status\":\"ok\"}" },
            { "name": "Error", "status": "Not Found", "code": 404, "body": "{\"error\":\"New owner not found\"}" }
          ]
        }
      ]
    },
